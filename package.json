{
  "name": "recados_api_arq",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only ./src/main/index.ts",
    "build": "rimraf dist && tsc",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "typeorm migration:create src/app/shared/migrations/CreateTable",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/database/typeorm.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/database/typeorm.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/database/typeorm.ts",
    "migration:run:test": "cross-env NODE_ENV=test npm run typeorm migration:run -- -d src/main/config/database.config.ts",
    "test": "jest --silent --verbose --noStackTrace --runInBand",
    "test:verbose": "jest --verbose --runInBand --detectOpenHandles",
    "test:coverage": "jest --silent --noStackTrace --runInBand --coverage",
    "test:watch": "jest --silent --noStackTrace --runInBand --watchAll",
    "test:integration": "cross-env NODE_ENV=test jest -c jest.integration.config.ts --silent --noStackTrace --runInBand",
    "test:integration:verbose": "cross-env NODE_ENV=test jest -c jest.integration.config.ts --verbose --runInBand --detectOpenHandles",
    "test:integration:coverage": "cross-env NODE_ENV=test jest -c jest.integration.config.ts --silent --noStackTrace --runInBand --coverage",
    "test:integration:watch": "cross-env NODE_ENV=test jest -c jest.integration.config.ts --silent --noStackTrace --runInBand --watchAll",
    "test:unit": "jest -c jest.unit.config.ts --silent --noStackTrace --runInBand",
    "test:unit:verbose": "jest -c jest.unit.config.ts --verbose --runInBand --detectOpenHandles",
    "test:unit:coverage": "jest -c jest.unit.config.ts --silent --noStackTrace --runInBand --coverage",
    "test:unit:watch": "jest -c jest.unit.config.ts --silent --noStackTrace --runInBand --watchAll",
    "test:setup": "rimraf database.sqlite3 && npm run migration:generate:test && npm run migration:run:test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.14.6",
    "@types/pg": "^8.6.6",
    "@types/reflect-metadata": "^0.1.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "cross-env": "^7.0.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typecript": "^0.0.1-security"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "faker": "^6.6.6",
    "ioredis": "^5.3.1",
    "jest": "^29.5.0",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "typeorm": "^0.3.10",
    "uuid": "^9.0.0"
  }
}
